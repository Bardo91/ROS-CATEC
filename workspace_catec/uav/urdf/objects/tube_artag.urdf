    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.57 0"/>
      <geometry>
        <cylinder radius="0.03" length="1.3"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="Tube_link">
    <material>PR2/Black</material>
    <turnGravityOff>true</turnGravityOff>
    <static>true</static>
  </gazebo>
  
  
  <link name="ARtag_link">
