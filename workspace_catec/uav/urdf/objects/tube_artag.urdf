    </inertial>
    <visual>
<!--      <origin xyz="0 0 -0.9" rpy="0 0 0"/>-->
      <origin xyz="-0.45 0 0.045" rpy="0 0 1.57"/>
      <geometry>
        <box size="0.04 0.04 0" />

      </geometry>
    </visual>
    <collision>
<!--      <origin xyz="0 0 -0.9" rpy="0 0 0"/>-->
      <origin xyz="-0.45 0 0.045" rpy="0 0 1.57"/>
      <geometry>
        <box size="0.04 0.04 0" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="ARtag_link">
    <material>Iri/ARtag</material>
    <turnGravityOff>true</turnGravityOff>
    <static>true</static>
  </gazebo>
  <joint name="tube_ARtag" type="fixed" >
	  <axis xyz="1 0 0" />
